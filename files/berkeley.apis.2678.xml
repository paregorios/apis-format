      <fileDesc>
         <titleStmt>
            <title>Account of expenses.</title>
         </titleStmt>
         <publicationStmt>
            <authority>APIS</authority>
            <idno type="apisid">berkeley.apis.2678</idno>
            <idno type="controlNo">(CU)2678</idno>
         </publicationStmt>
         <sourceDesc>
            <msDesc>
               <msIdentifier>
                  <idno type="invNo">P.Tebt.Frag.14,211+14,214,215+14,232+14,241+14,261+14,262+14,263+14,264+14,271</idno>
               </msIdentifier>
               <msContents>
                  <summary>Account of expenses mentioning small amounts for women and men. Frags. 14,261 and 14,263 (?) give overview of account.</summary>
                  <msItemStruct>
                     <note type="related">Related to P.Tebt.Frag.14,435 and 14,436</note>
                     <textLang mainLang="egy-Egyd">Demotic; verso of Frag. 145,215 in Greek.</textLang>
                  </msItemStruct>
               </msContents>
               <physDesc>
                  <objectDesc>
                     <supportDesc>
                        <support>9 papyri ; Frag. 14,211, 17 x 14 cm. (frame 1); Frag. 14,215, 20 x 13 cm. (frame 2); Frag. 14,232, 13 x 13 cm. (frame 3); Frag. 14,241, 16.5 x 16 cm. (frame 4); Frag. 14,261, 15 x 21 cm. (frame 5); Frag. 14,262, 15 x 21 cm. (frame 6); Frag. 14,263, 16 x 21 cm. (frame 7); Frag. 14,264, 29.5 x 14.5 cm. (frame 8); Frag. 14,271, 12.5 x 23 cm. (frame 9)</support>
                        <condition>
                           <ab type="conservation">Most gesso and paint removed</ab>
                        </condition>
                     </supportDesc>
                     <layoutDesc>
                        <layout>
                           <ab type="lines">17 cols. on recto along the fibers; 17+ cols. on verso across the fibers.</ab>
                           <ab type="recto-verso">Verso: continuation of text</ab>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc>
                     <p>Same or similar hands on various fragments, but height varies.</p>
                  </handDesc>
               </physDesc>
               <history>
                  <origin>
                     <origDate notBefore="-0150" notAfter="-0030">Between ca. 150 and ca. 30 B.C.?</origDate>
                  </origin>
                  <provenance>
                     <p>Mummy cartonnage at Tebtunis</p>
                  </provenance>
               </history>
            </msDesc>
         </sourceDesc>
